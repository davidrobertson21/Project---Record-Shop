<h1>Artists</h1>
<br/>
<p><a href='/artists/new'> New Artist</a></p>
<br/>
<hr/>


<% @artists.each do |artist| %>
  <p><%= artist.name() %> </p>


<p><a href='/artists/<%= artist.id %>'> Show Artist</a></p>
<p><a href='/artists/<%= artist.id %>/edit'> Edit Artist</a></p>
<form method="post" action="/artists/<%= artist.id %>/delete">
<input type="submit" value="Delete Artist"/>
</form>

  <hr/>
<% end %>